{
    "name": "raid/core-action",
    "description": "Raid Core Action Package",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "keywords": [
        "laravel",
        "raid",
        "core",
        "action"
    ],
    "require": {
        "php": "^8.2",
        "raid/core-enum": "*",
        "raid/core-gate": "*",
        "raid/core-command": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Raid\\Core\\Action\\": "src"
        }
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint --config pint.json "
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Raid\\Core\\Action\\Providers\\ActionServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "mohamed_khedr",
            "email": "mohamedkhedr700@gmail.com"
        }
    ]
}
